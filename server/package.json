{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p ./tsconfig.json",
    "start": "cross-env NODE_ENV=prod node ./dist/src/main.js",
    "dev": "cross-env NODE_ENV=dev ts-node-dev --respawn --clear ./src/main.ts",
    "lint": "eslint . && prettier --check src/*.ts",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit",
    "migrate": "sh ./prisma/migrate.sh",
    "seed": "sh ./prisma/seed.sh",
    "studio": "sh ./prisma/studio.sh"
  },
  "prisma": {
    "seed": "ts-node-dev ./prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.13",
    "@types/supertest": "6.0.2",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "prisma": "5.20.0",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-node-dev": "2.0.0",
    "ts-standard": "12.0.2",
    "typescript": "5.6.2"
  },
  "dependencies": {
    "@prisma/client": "5.20.0",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "env-var": "7.5.0",
    "express": "4.21.0",
    "rimraf": "6.0.1",
    "zod": "3.23.8"
  }
}
